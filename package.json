{
  "name": "amd-angular-ui-src",
  "version": "0.0.1",
  "description": "An Angular UI component framework by AMD Core",
  "scripts": {
    "build": "gulp",
    "lint": "gulp lint",
    "test": "karma start karma.conf.js",
    "test:e2e": "protractor protractor.conf.js",
    "test:e2e:browserstack": "protractor config/protractor-browserstack.conf.js",
    "webdriver:update": "webdriver-manager update",
    "postinstall": "npm run webdriver:update",
    "clean": "rimraf build",
    "start": "webpack-dev-server --progress",
    "start:demo": "npm run start",
    "build:demo": "webpack --progress",
    "start:demo:aot": "npm run start -- --config=demo/config/webpack.aot --env=production",
    "build:demo:aot": "npm run build:demo -- --config=demo/config/webpack.aot --env=production",
    "build:demo:aot:server": "npm run build:demo -- --config=demo/config/webpack.aot --env=production --output-public-path=public/",
    "build:demo:server": "npm run build:demo -- --config=demo/config/webpack.server --env=production",
    "start:demo:server": "node demo/build/server"
  },
  "contributors": [
